package anavs;

public class MultiplikationMatrizen {
static int A[][];
static int B[][];
private int C[][];
MultiplikationMatrizen(){
	A[0][0]=1;
	A[0][1]=-2;
	A[0][2]=3;
	A[0][3]=4;
	A[1][0]=-2;
	A[1][1]=3;
	A[1][2]=0;
	A[1][3]=1;
	A[2][0]=4;
	A[2][1]=-1;
	A[2][2]=2;
	A[2][3]=1;
	A[3][0]=-2;
	A[3][1]=1;
	A[3][2]=3;
	A[3][3]=-1;
	
	B[0][0]=2;
	B[0][1]=-4;
	B[0][2]=-1;
	B[0][3]=1;
	B[1][0]=-1;
	B[1][1]=1;
	B[1][2]=-2;
	B[1][3]=2;
	B[2][0]=5;
	B[2][1]=0;
	B[2][2]=3;
	B[2][3]=-2;
	B[3][0]=1;
	B[3][1]=-2;
	B[3][2]=1;
	B[3][3]=0;
	
	int Aa[][] = {{1, -2, 3, 4}, {-2, 3, 0, 1}, {4, -1, 2, 1}, {-2, 1, 3, -1}};
    //int B[][] = {{2, -4, -1, 1}, {-1, 1, -2, 2}, {5, 0, 3, -2}, {1, -2, 1, 0}};
    int C[][] = new int[4][4];
}
	public static void main(String[] args) {
		System.out.print("A: ");
		for(int j =0;j<4;j++){
			for(int i=0;i<4;i++){
			System.out.print(Aa[i][j]);
			}
		}
	}

}
